{
  "name": "dates-list-generator",
  "version": "1.0.0",
  "description": "Generate dates list from date to date, excluding certain days(e.g. holidays)",
  "main": "index.html",
  "scripts": {
    "pug": "pug index.pug",
    "dev": "npm-run-all --parallel serve watch",
    "serve": "browser-sync start --server --files \"*.html\"",
    "watch": "onchange \"index.pug\" -- npm run pug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgg/dates-list-generator.git"
  },
  "author": "lgg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgg/dates-list-generator/issues"
  },
  "homepage": "https://github.com/lgg/dates-list-generator#readme",
  "devDependencies": {
    "browser-sync": "^2.15.0",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6"
  }
}
